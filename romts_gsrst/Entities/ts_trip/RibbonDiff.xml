    </DisplayRules>
    <EnableRules>
      <EnableRule Id="Mscrm.IsValidForHierarchyPageInUC">
        <CustomRule FunctionName="XrmCore.Rules.HierarchyView.isValidForHierarchyPageInUC" Library="$webresource:Main_system_library.js" />
